<div class="header">
    <nav>
        <ul class="main-nav">
            <li><a href="/api">API</a></li>
        </ul>
    </nav>
</div>
<h1>CHECK YOUR ACCESS</h1>
<table class="ip-table">
    <tbody>
        <tr class="ip-tr">
            <td class="ip-td1">IP Address</td>
            <td class="ip-td2"><%=@remote_ip%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">ACCESS TIME</td>
            <td class="ip-td2"><%=@access_time%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">HTTP HOST</td>
            <td class="ip-td2"><%=@http_host%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">HTTP VERSION</td>
            <td class="ip-td2"><%=@http_version%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">REQUEST METHOD</td>
            <td class="ip-td2"><%=@request_method%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">REQUEST URI</td>
            <td class="ip-td2"><%=@request_uri%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">BROWSER</td>
            <td class="ip-td2"><%="#{@browser} #{@b_version}"%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">OS</td>
            <td class="ip-td2"><%=@platform%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">USER AGENT</td>
            <td class="ip-td2"><%=@user_agent%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">HTTP ACCEPT LANGUAGE</td>
            <td class="ip-td2"><%=@http_accept_language%></td>
        </tr>
        <tr class="ip-tr">
            <td class="ip-td1">REMOTE HOST</td>
            <td class="ip-td2"><%=@hrenite_host%></td>
        </tr>
    </tbody>
</table>